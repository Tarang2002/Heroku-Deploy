{
  "name": "Heroku-Deploy",
  "version": "1.0.0",
  "description": "Heroku Deployment Demo",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && tsc"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.2"
  }
}
